/* =========================
   Video Section
========================= */

.video-section {
    display: flex;
    align-items: center;
    justify-content: center;

    padding: 80px 0;       /* vertical spacing */
    width: 70%;
    margin: 0 auto;       /* centers the section itself */

    box-sizing: border-box;
}

.video {
    position: relative;
    overflow: hidden;
}

.full__width__padding {
    padding-left: 60px;
    padding-right: 60px;
}

@media (max-width: 991px) {
    .full__width__padding {
        padding-left: 20px;
        padding-right: 20px;
    }
}

/* Video Image Wrapper */
.video__img {
    position: relative;
    border-radius: 20px;
    overflow: hidden;
}

/* Main Image */
.video__img > img {
    width: 100%;
    height: auto;
    display: block;
    border-radius: 20px;
}

/* Small Floating Image */
.video__small__img {
    position: absolute;
    bottom: 30px;
    right: 30px;
    max-width: 180px;
    border-radius: 14px;
    overflow: hidden;
    box-shadow: 0 15px 40px rgba(0,0,0,0.25);
}

.video__small__img img {
    width: 100%;
    display: block;
}

/* Video Button */
.video__button {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.video__card--link {
    width: 90px;
    height: 90px;
    background: #ffffff;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 20px 50px rgba(0,0,0,0.25);
    transition: all 0.3s ease;
}

.video__card--link:hover {
    transform: scale(1.1);
    background: #F8422E;
}

.video__card--link:hover svg path {
    fill: #ffffff;
}

/* Overlay Text */
.video__text {
    position: absolute;
    z-index: 2;
}

.video__text h2 {
    font-size: 100px;
    font-weight: 800;
    color: rgba(0, 0, 0, 0.15);
    letter-spacing: 6px;
    text-transform: uppercase;
    margin: 0;
    pointer-events: none;
}

/* Text Positions */
.video__text--1 {
    top: 40px;
    left: 50px;
}

.video__text--2 {
    bottom: 40px;
    right: 50px;
}

/* Responsive Text */
@media (max-width: 991px) {
    .video__text h2 {
        font-size: 60px;
    }

    .video__text--1 {
        top: 20px;
        left: 20px;
    }

    .video__text--2 {
        bottom: 20px;
        right: 20px;
    }

    .video__card--link {
        width: 70px;
        height: 70px;
    }
}

@media (max-width: 575px) {
    .video__text h2 {
        font-size: 42px;
    }

    .video__small__img {
        display: none;
    }
}




